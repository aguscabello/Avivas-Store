<div id="<%= dom_id sale %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= sale.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Date:</strong>
    <%= sale.date %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Total:</strong>
    <%= sale.total %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Canceled:</strong>
    <%= sale.canceled %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Client name:</strong>
    <%= sale.client_name %>
  </p>

</div>
