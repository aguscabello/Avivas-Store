<div class="w-full">


  <% content_for :title, "Users" %>

  <% if user_signed_in? && current_user.admin? %>
    <div class="flex justify-between items-center mb-6">
      <h1 class="font-bold text-4xl">Users</h1>
      <%= link_to "New user", new_admin_user_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
    </div>
  <% end %>

   <%= render 'shared/table', users: @users %>

</div>
